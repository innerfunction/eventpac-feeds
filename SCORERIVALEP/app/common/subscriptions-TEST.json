{
    "updateInterval": 300,
    "resetContentAtStart": true,
    "refreshPeriodicSubscriptions": true,
    "initialContentPath": "common/subs",
    "subscriptions": {
        "nagp": {
            "id":           "nagp",
            "type":         "IncrementalSubscription",
            "url":          "`http://localhost:8123/?feed={feed}&since={since}",
            "periodic":     true,
            "zip":          "@app:/common/subs/nagp/content.zip"
        }
    }
}

